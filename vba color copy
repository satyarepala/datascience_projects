Sub CopyColorsOnly()
    Dim wsSource As Worksheet, wsTarget As Worksheet
    Dim rngSource As Range, rngTarget As Range
    Dim cell As Range

    ' Set your sheets (change names as needed)
    Set wsSource = ThisWorkbook.Sheets("Sheet1") ' Source sheet with colors
    Set wsTarget = ThisWorkbook.Sheets("Sheet2") ' Target sheet where colors will be copied

    ' Define the range (Adjust as per your data)
    Set rngSource = wsSource.Range("A1:Z100") ' Adjust to match your colored range
    Set rngTarget = wsTarget.Range("A1:Z100") ' Make sure it matches the source range size

    ' Loop through each cell and copy background color
    For Each cell In rngSource
        rngTarget.Cells(cell.Row, cell.Column).Interior.Color = cell.Interior.Color
    Next cell

    MsgBox "Colors copied successfully!", vbInformation
End Sub
